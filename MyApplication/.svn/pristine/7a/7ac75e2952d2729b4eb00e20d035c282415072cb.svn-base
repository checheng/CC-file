<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="button_mainlayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="map_button">
        <item name="android:background">#00000000</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="Transparent_button">
        <item name="android:background">#00000000</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="SwitchCompat.Text" parent="Theme.AppCompat.Light">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">9dp</item>
    </style>
    <style name="SwitchCompat.Control" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">@color/lightpink</item>
        <item name="colorSwitchThumbNormal">@color/lightblue</item>
        <item name="android:colorForeground">@color/lightblue</item>
    </style>
    <style name="Text_black">
        <item name="android:textColor">@color/black</item>
    </style>

</resources>
